{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch local",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      // Remove if not use
      //"envFile": "${workspaceFolder}/.env"
    },
    {
      "name": "Attach remote process",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "port": 2356,
      "host": "127.0.0.1",
      "apiVersion": 2,
      "showLog": true
    }
  ]
}